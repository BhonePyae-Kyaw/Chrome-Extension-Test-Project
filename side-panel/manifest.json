{
  "manifest_version": 3,
  "name": "My side panel extension",
  "version": "1.0",
  "permissions": ["sidePanel"],
  "side_panel": {
    "default_path": "/side-panel.html"
  },
  "action": {
    "default_title": "Click to open panel"
  },
  "icons": {
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
